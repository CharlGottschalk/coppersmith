{
  "name": "coppersmith",
  "version": "1.0.1",
  "description": "Simple static site builder using markdown",
  "main": "cs/index.js",
  "scripts": {
    "test": "test"
  },
  "devDependencies": {
    "gulp": "^3.9.1",
    "gulp-jshint": "^2.0.1",
    "jshint": "^2.9.3",
    "jshint-stylish": "^2.2.1",
    "laravel-elixir": "^6.0.0-11"
  },
  "dependencies": {
    "async": "^2.0.1",
    "chalk": "^1.1.3",
    "ftp": "^0.3.10",
    "handlebars": "^4.0.5",
    "inquirer": "^1.1.3",
    "metalsmith": "^2.2.0",
    "metalsmith-assets": "^0.1.0",
    "metalsmith-collections": "^0.7.0",
    "metalsmith-drafts": "0.0.1",
    "metalsmith-headings": "^0.1.0",
    "metalsmith-ignore": "^0.1.2",
    "metalsmith-in-place": "^1.4.4",
    "metalsmith-layouts": "git://github.com/superwolff/metalsmith-layouts.git#master",
    "metalsmith-markdown": "^0.2.1",
    "metalsmith-permalinks": "^0.5.0",
    "pretty-error": "^2.0.0",
    "slugify": "^1.0.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/charlgottschalk/doc-smith.git"
  },
  "keywords": [
    "coppersmith",
    "metalsmith",
    "markdown",
    "doc",
    "docs",
    "documentation",
    "builder",
    "static",
    "site",
    "generator",
    "charl gottschalk"
  ],
  "author": "Charl Gottschalk (https://github.com/CharlGottschalk)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/charlgottschalk/coppersmith/issues"
  },
  "homepage": "https://github.com/charlgottschalk/coppersmith#readme",
  "bin": {
    "copper": "./cs/cmd/index.js"
  },
  "jshintConf": {
    "globals": {
      "require": true,
      "console": true,
      "exports": true,
      "process": true,
      "__dirname": true
    },
    "maxparams": 5,
    "maxdepth": 5,
    "maxstatements": 25,
    "maxcomplexity": 10,
    "es5": true,
    "browser": true,
    "boss": false,
    "curly": false,
    "debug": false,
    "devel": false,
    "eqeqeq": true,
    "evil": true,
    "forin": false,
    "immed": true,
    "laxbreak": false,
    "newcap": true,
    "noarg": true,
    "noempty": false,
    "nonew": false,
    "nomen": false,
    "onevar": true,
    "plusplus": false,
    "regexp": false,
    "undef": true,
    "sub": true,
    "strict": true,
    "white": true,
    "unused": true
  }
}
